    "quasar": "^2.18.5",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@quasar/vite-plugin": "^1.10.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.18.6",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "cypress": "^15.3.0",
    "jiti": "^2.6.1",
    "jsdom": "^27.0.0",
